					<key>COMMAND_STRING</key>
					<string># Encrypt
# @see: https://apple.stackexchange.com/questions/50441/using-automator-or-applescript-to-encrypt-decrypt-with-openssl

password="$1"
shift
if [[ -z "$password" ]]; then
    exit 1
fi

directory="$(dirname $1)"
if [[ ! -d "$directory" ]]; then
    exit 1;
fi

zip_file="$(mktemp).zip"
zip -r "$zip_file" "$@"

openssl enc -aes-256-cbc -md sha512 -pbkdf2 -iter 1000000 -salt -in "$zip_file" -out "$zip_file.enc" -pass pass:"$password"
mv "$zip_file.enc" "$directory/encrypted-$(date +"%Y%m%d%H%M%S").zip.enc"</string>
